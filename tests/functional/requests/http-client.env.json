{
  "$shared": {
    "$default_headers": {
      "Accept": "application/json",
      "Content-Type": "application/json",
      "Host": "httpbin.org"
    },
    "DEFAULT_TIMEOUT": 1
  },
  "dev": {
    "$default_headers": {
      "Test-Header": "Test-Value",
      "Test-Header-2": "Test-Value-2",
      "Test-Header-3": "{{pokemon}}"
    },
    "PASSWORD": "bananas",
    "Recursive-Var": "{{USERNAME}}",
    "Security": {
      "Auth": {
        "GAPI": {
          "Acquire Automatically": true,
          "Auth URL": "https://auth.url",
          "Client ID": "client_id",
          "Custom Request Parameters": {
            "access_type": {
              "Use": "In Auth Request",
              "Value": "offline"
            },
            "audience": "kulala_api"
          },
          "Grant Type": "Password",
          "Redirect URL": "http://localhost:8080",
          "Revoke URL": "",
          "Scope": "scope:sample",
          "Token URL": "https://token.url",
          "Type": "OAuth2",
          "Use ID Token": false,
          "Username": "test@mail.com"
        }
      }
    },
    "USERNAME": "gorillamoe",
    "auth_header": "Authorization: Basic Base64encodedstring==",
    "deep": {
      "nested": {
        "var": "deep-nested-variable"
      }
    },
    "pokemon": "ditto"
  },
  "prod": {
    "PASSWORD": "polo",
    "USERNAME": "marco",
    "auth_header": "Authorization: Basic Base64encodedstring==",
    "pokemon": "pikachu"
  }
}
